package es.florida.T1FitArch;
import java.io.File;
public class Sec1_3 {
	//Introduir una comprovaci√≥ en el programa anterior per determinar si el directori existeix.
	public static void main(String[] args) {
		
		File file = new File("../src");
		
		if(file.exists()) {
			System.out.println("No existe");
			
			
		}else {
			
			if(file.isDirectory()) {
				
				System.out.println("Existe el directio");
				
			}else
			{
				System.out.println("No existe como directorio");
				
			}
		}
			
		
		
		
	}
}
