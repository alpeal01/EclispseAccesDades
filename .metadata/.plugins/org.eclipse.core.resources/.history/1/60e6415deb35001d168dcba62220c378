package es.florida.T1FitArch;
import java.io.File;

//Modifica el programa anterior per que tinga en compte que si no se li passa ninguna extensiÃ³
//com a parÃ metre, mostre tot el contingut del directori.

public class Sec1_6 {

	public static void main(String[] args) {
		
		File file = new File(args[0]);
		
		
		
		
			
			String [] lista  = file.list();
			
			for (String data : lista) {
				
				if( args[1].length() > 0 && data.contains(args[1])){
					
					System.out.println(data);
					
					
				}
				
			}
			
		
		
	}

}
